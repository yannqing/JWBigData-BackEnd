<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxjw.jwbigdata.mapper.JwFieldMapper">

    <resultMap id="BaseResultMap" type="com.wxjw.jwbigdata.domain.JwField">
            <result property="fieldName" column="field_name" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="dataType" column="data_type" jdbcType="VARCHAR"/>
            <result property="tableId" column="table_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        field_name,description,data_type,
        table_id
    </sql>
</mapper>
